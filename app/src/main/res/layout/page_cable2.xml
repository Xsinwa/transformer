            android:layout_marginLeft="20dp"
            android:paddingTop="10dp"
            android:paddingBottom="5dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="变频电源容量(kW):"
                android:textSize="16sp"/>

            <TextView
